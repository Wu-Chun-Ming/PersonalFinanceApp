{
  "name": "personalfinanceapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "DARK_MODE=media expo start --web",
    "lint": "expo lint",
    "test": "jest --watch --coverage=false --verbose",
    "test:unit": "jest --watch --coverage=false --verbose __tests__/unit",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest --verbose",
    "testFinal:unit": "jest  --verbose __tests__/unit --coverageDirectory=coverage/unit",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "dependencies": {
    "@expo/html-elements": "^0.4.2",
